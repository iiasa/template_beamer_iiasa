\mode<presentation>

% Set colors 
% HTML colors list: http://www.computerhope.com/htmcolor.htm
\definecolor{iiasadarckblue}{HTML}{15317E}
\definecolor{iiasalightblue}{HTML}{1569C7}
\definecolor{iiasalightgray}{HTML}{EEEEEE}
\definecolor{iiasadarckgray}{HTML}{686868}
\definecolor{iiasacranberry}{HTML}{9F000F}

% Settings 
\setbeamercolor*{title page}    {fg=iiasadarckblue}
\setbeamercolor*{title}         {fg=iiasadarckblue}
\setbeamercolor*{subtitle}      {fg=iiasadarckblue}
\setbeamercolor*{author}        {fg=iiasadarckblue}
\setbeamercolor*{institute}     {fg=iiasadarckblue}
\setbeamercolor*{date}          {fg=iiasadarckblue!60!}
\setbeamercolor*{frametitle}    {fg=iiasadarckblue}
\setbeamercolor*{framesubtitle} {fg=iiasadarckblue}

\setbeamercolor*{block title}{bg=iiasadarckblue,fg=white}
\setbeamercolor*{block title alerted}{bg=iiasacranberry,fg=white}
\setbeamercolor*{block title example}{bg=iiasalightblue,fg=white}

\setbeamercolor*{block body} {bg=iiasalightgray,fg=iiasadarckgray}
%\setbeamercolor*{alerted body} {bg=iiasalightgray,fg=iiasadarckgray}
%\setbeamercolor*{example body} {bg=iiasalightgray,fg=iiasadarckgray}

\setbeamercolor*{normal text}{bg=white,fg=iiasadarckgray}
\setbeamercolor*{section in head/foot}{bg=iiasadarckblue,fg=white}

\mode
<all>